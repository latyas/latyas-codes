import urllib
import sys
from bs4 import BeautifulSoup

def etymology(key):
	page = urllib.urlopen("http://www.etymonline.com/index.php?term=" + key).read().decode("utf-8")
	try:
		soup = BeautifulSoup(page)
		foo = soup.findAll('div', id='dictionary')
		rep = re.compile('<img.*"/>?')
		ret = rep.sub('',str(foo[0]))
		ret = ret.replace('</div>','')
		ret = ret.replace('<div id="dictionary">','')
		
		return ret
	except:
		pass
	return None

if __name__ == '__main__':
	if len(sys.argv) != 2:
		print 'Usage: %s [WORD]' % sys.argv[0]
	foo = etymology(sys.argv[1])
	if foo:
		print foo
	else:
		print 'Not Found'
